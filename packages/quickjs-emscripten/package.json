{
  "name": "quickjs-emscripten",
  "version": "0.25.0-rc.3",
  "sideEffects": false,
  "license": "MIT",
  "type": "commonjs",
  "keywords": [
    "eval",
    "quickjs",
    "vm",
    "interpreter",
    "runtime",
    "safe",
    "emscripten",
    "wasm"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/justjake/quickjs-emscripten"
  },
  "author": {
    "name": "Jake Teton-Landis",
    "url": "https://jake.tl"
  },
  "files": [
    "dist/**/*",
    "!dist/*.test.js",
    "!dist/*.tsbuildinfo"
  ],
  "scripts": {
    "build": "npx tsup",
    "tsc": "npx tsc -p . --noEmit",
    "test:vi": "VITEST=1 npx vitest run",
    "test:node": "npx tsc -p . && node -r source-map-support/register --test ./.output/*node-test*.*js",
    "test": "npm run test:vi && npm run test:node"
  },
  "types": "./dist/index.d.ts",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./variants": {
      "types": "./dist/variants.d.ts",
      "import": "./dist/variants.mjs",
      "require": "./dist/variants.js"
    },
    "./package.json": "./package.json"
  },
  "imports": {
    "#variants/debug": {
      "types": "@jitl/quickjs-node-debug-sync-wasm",
      "browser": "@jitl/quickjs-browser-debug-sync-singlefile",
      "import": "@jitl/quickjs-node-debug-sync-wasm",
      "require": "@jitl/quickjs-node-debug-sync-wasm"
    },
    "#variants/debug-asyncify": {
      "types": "@jitl/quickjs-node-debug-asyncify-wasm",
      "browser": "@jitl/quickjs-browser-debug-asyncify-singlefile",
      "import": "@jitl/quickjs-node-debug-asyncify-wasm",
      "require": "@jitl/quickjs-node-debug-asyncify-wasm"
    },
    "#variants/release": {
      "types": "@jitl/quickjs-node-release-sync-wasm",
      "browser": "@jitl/quickjs-browser-release-sync-singlefile",
      "import": "@jitl/quickjs-node-release-sync-wasm",
      "require": "@jitl/quickjs-node-release-sync-wasm"
    },
    "#variants/release-asyncify": {
      "types": "@jitl/quickjs-node-release-asyncify-wasm",
      "browser": "@jitl/quickjs-browser-release-asyncify-singlefile",
      "import": "@jitl/quickjs-node-release-asyncify-wasm",
      "require": "@jitl/quickjs-node-release-asyncify-wasm"
    }
  },
  "dependencies": {
    "@jitl/quickjs-browser-debug-asyncify-singlefile": "workspace:*",
    "@jitl/quickjs-browser-debug-sync-singlefile": "workspace:*",
    "@jitl/quickjs-browser-release-asyncify-singlefile": "workspace:*",
    "@jitl/quickjs-browser-release-sync-singlefile": "workspace:*",
    "@jitl/quickjs-node-debug-asyncify-wasm": "workspace:*",
    "@jitl/quickjs-node-debug-sync-wasm": "workspace:*",
    "@jitl/quickjs-node-release-asyncify-wasm": "workspace:*",
    "@jitl/quickjs-node-release-sync-wasm": "workspace:*",
    "quickjs-emscripten-core": "workspace:*"
  },
  "engines": {
    "node": ">=14.6.0"
  },
  "stableVersion": "0.24.0"
}
