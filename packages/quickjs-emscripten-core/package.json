{
  "name": "quickjs-emscripten-core",
  "version": "0.25.0",
  "sideEffects": false,
  "license": "MIT",
  "type": "commonjs",
  "keywords": [
    "eval",
    "quickjs",
    "vm",
    "interpreter",
    "runtime",
    "safe",
    "emscripten",
    "wasm"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/justjake/quickjs-emscripten"
  },
  "files": [
    "dist/**/*",
    "!dist/*.test.js",
    "!dist/*.tsbuildinfo"
  ],
  "exports": {
    "import": "./dist/index.mjs",
    "require": "./dist/index.js",
    "types": "./dist/index.d.ts"
  },
  "scripts": {
    "tsc": "npx tsc -p . --noEmit",
    "test": "npx vitest run",
    "build": "npx tsup"
  },
  "dependencies": {
    "@jitl/quickjs-ffi-types": "workspace:*"
  }
}
