# https://gittup.org/tup/manual.html

# Host: x86_64 linux
CONFIG_LTO_HOST=y
CONFIG_CC_HOST=gcc
CONFIG_AR_HOST=gcc-ar
CONFIG_DEFINES_HOST=-D_GNU_SOURCE -D__linux__
CONFIG_LDFLAGS_HOST=-rdynamic
CONFIG_LIBS_HOST=-ldl -lpthread

# Target: x86_64 linux
CONFIG_LTO_TARGET=y
CONFIG_CC_TARGET=gcc
CONFIG_AR_TARGET=gcc-ar
CONFIG_DEFINES_TARGET=-D_GNU_SOURCE -D__linux__
CONFIG_LDFLAGS_TARGET=-rdynamic
CONFIG_LIBS_TARGET=-ldl -lpthread
