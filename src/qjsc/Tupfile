# https://gittup.org/tup/manual.html
include_rules

include ../archives/core/deps.tup

DEPS_HOST += ../quickjs-libc/quickjs-libc.host.o
DEPS_TARGET += ../quickjs-libc/quickjs-libc.target.o

: qjsc.c $(DEPS_HOST) |> !compile_host_c_program |> qjsc.host
: qjsc.c $(DEPS_TARGET) |> !compile_target_c_program |> qjsc.target
