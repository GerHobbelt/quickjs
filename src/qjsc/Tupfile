include_rules

DEPS = ../cutils/cutils.o
DEPS += ../libbf/libbf.o
DEPS += ../libregexp/libregexp.o
DEPS += ../libunicode/libunicode.o
DEPS += ../quickjs/quickjs.o
DEPS += ../quickjs-libc/quickjs-libc.o

ifdef WIN32
  OUTPUT = qjsc.exe
else
  OUTPUT = qjsc
endif

: qjsc.c $(DEPS) |> !compile_c_program |> $(OUTPUT)
